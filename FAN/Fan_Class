#Create a Fan Class and assign the speed
class FAN:
    Slow = 1
    Medium= 2
    Fast = 3
    
    #Define init 
    def __init__(self, speed = FAN.Slow, radius = 5 , color = 'blue', on = false) :
        self.speed = speed
        self.radius = radius
        self.color = color
        self.on = on
    




    
    